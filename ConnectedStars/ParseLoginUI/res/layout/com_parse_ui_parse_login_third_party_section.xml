<merge xmlns:android="http://schemas.android.com/apk/res/android">

   <Button
        android:id="@+id/facebook_login"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_gravity="left"
       android:layout_weight="0.25"
        android:background="@null"
    android:drawableLeft="@drawable/fb"
    android:layout_marginLeft="30dp"
       android:layout_marginRight="12dp"
       />

    <Button
    android:id="@+id/twitter_login"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_weight="0.25"
        android:layout_toRightOf="@id/facebook_login"
        android:background="@null"
        android:drawableLeft="@drawable/twitt"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
    />

    <Button
        android:id="@+id/google_Login"

        android:layout_gravity="right"
        android:layout_weight="0.25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:drawableLeft="@drawable/gog"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        />

    <Button
        android:id="@+id/linkedin_Login"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_weight="0.25"
        android:layout_toRightOf="@id/facebook_login"
        android:background="@null"
        android:drawableLeft="@drawable/linkedin32"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="10dp"
        />

